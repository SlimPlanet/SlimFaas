{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Cors": {
    "Origins": [ "*" ],
    "Methods": [ "*" ],
    "Headers": [ "*" ],
    "Expose":  [ "WWW-Authenticate" ],
    "Credentials": false,
    "MaxAgeMinutes": 10
  },
  "OpenTelemetry": {
    "Enable": true,
    "Endpoint": "http://localhost:4317",
    "ServiceName": "SlimFaasMcp",
    "EnableConsoleExporter": true
  }
}
