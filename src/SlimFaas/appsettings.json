{
    "Logging": {
        "LogLevel": {
            "Default": "Warning",
            "Microsoft.AspNetCore": "Error",
            "SlimData": "Error",
            "SlimFaas": "Error",
            "DotNext": "Error",
            "DotNext.Net.Cluster": "Error",
            "DotNext.Net.Cluster.Consensus.Raft": "Error"
        }
    },
    "UseKubeConfig": false,
    "AllowedHosts": "*",
    "Kestrel": {
        "Limits": {
            "KeepAliveTimeout": "00:05:00",
            "RequestHeadersTimeout": "00:05:00",
            "MaxRequestBodySize": 524288000,
            "MinRequestBodyDataRate": {
                "BytesPerSecond": 240,
                "GracePeriod": "00:00:10"
            }
        }
    },
    "OpenTelemetry": {
        "Enable": false,
        "Endpoint": "",
        "ServiceName": "",
        "EnableConsoleExporter": false,
        "ExcludedUrls": ["/health", "/metrics"]
    },
    "Data": {
        "DefaultVisibility": "Private"
    }
}
