{
    "Logging": {
        "LogLevel": {
            "Default": "Error",
            "Microsoft.AspNetCore": "Error"
        }
    },
    "UseKubeConfig": true,
    "OpenTelemetry": {
        "Enable": true,
        "Endpoint": "http://localhost:4317",
        "ServiceName": "SlimFaas",
        "EnableConsoleExporter": true,
        "ExcludedUrls": ["/health"]
    },
    "SlimFaas": {
        "Orchestrator": "Kubernetes",
        "AllowUnsecureSsl": true
    }
}
